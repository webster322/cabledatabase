<div class="container-fluid">
  <div class="row content">
    <div class="col-sm-3 sidenav">
      <ul class="nav nav-pills nav-stacked">
        <li class="active"><%=link_to 'Wszyskie obiekty', locations_index_path %></li>
        <li><%=link_to 'Dodaj nowy obiekt', locations_new_path %></li>
      </ul>
    </div>

    <div class="col-sm-9">
      <% if flash[:notice] %>
        <div class="alert alert-success" role="alert"><%= flash[:notice] %></div>
      <% end %>
      <table class="table">
        <thead>
          <th>Nazwa obiektu</th>
        </thead>
        <tbody>
          <%= @locations.each do |value| %>
            <tr>
              <td><%= value['name'] %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
