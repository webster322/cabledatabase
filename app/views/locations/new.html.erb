<div class="container-fluid">
  <div class="row content">
    <div class="col-sm-3 sidenav">
      <ul class="nav nav-pills nav-stacked">
        <li class="active"><%=link_to 'Wszyskie kable', wires_view_path %></li>
        <li><%=link_to 'Dodaj nowy kabel', wires_new_path %></li>
      </ul>
    </div>

    <div class="col-sm-9">
      <h1>Dodawanie obiektu</h1>
      <%= form_for @location do |f| %>
        <div class="form-group">
          <%= f.label :name, "Nazwa" %>
          <%= f.text_field :name, class: "form_control" %>
        </div>
        <%= f.submit "Dodaj obiekt", class: "btn btn-default" %>
      <% end %>
    </div>
  </div>
</div>
