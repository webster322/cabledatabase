<div class="container-fluid">
  <div class="row content">
    <div class="col-sm-3 sidenav">
      <ul class="nav nav-pills nav-stacked">
        <li class="active"><%=link_to 'Wszyskie kable', wires_view_path %></li>
        <li><%=link_to 'Dodaj nowy kabel', wires_new_path %></li>
      </ul>
    </div>

    <div class="col-sm-9">

      <table class="table">
        <thead>
          <th>Nazwa kabla</th>
          <th>Obiekt początkowy</th>
          <th>Obiekt końcowy</th>
        </thead>
        <tbody>
          <%= @cable.each do |value| %>
            <tr>
              <td><%= cable['name'] %></td>
              <td><%= cable['from'] %></td>
              <td><%= cable['to'] %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
